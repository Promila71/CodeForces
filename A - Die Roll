#include <iostream>
#include <algorithm>

using namespace std;

int main()
{

    int Y,W,R;
    cin>>Y>>W;
    R=max(Y,W);
    if(R==1)
        cout<<"1/1"<<endl;
    else if(R==2)
        cout<<"5/6"<<endl;
    else if(R==3)
        cout<<"2/3"<<endl;
    else if(R==4)
        cout<<"1/2"<<endl;
    else if(R==5)
        cout<<"1/3"<<endl;
    else if(R==6)
        cout<<"1/6"<<endl;
    else 
        cout<<"0/1"<<endl;
    return 0;
}
